{
	"name": "torasenstore",
	"version": "0.1.0",
	"description": "Custom functionality for Torasen Store.",
	"author": "Red Fern",
	"main": "build/index.js",
	"scripts": {
		"build": "wp-scripts build --webpack-src-dir=editor --config=webpack.config.js",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start --webpack-src-dir=editor --config=webpack.config.js",
		"watch": "npx tailwindcss -i ./css/main.css -o ./build/css/main.css --watch"
	},
	"devDependencies": {
		"@wordpress/scripts": "^27.7.0",
		"tailwindcss": "^3.4.4"
	},
	"dependencies": {
		"@headlessui/react": "^2.1.0"
	}
}
